pkgname=firmware-samsung-espresso10
pkgver=1
pkgrel=1
_commit="89fe5020dbaf2920dece954ce86aa2edf3389852"
pkgdesc="Samsung Galaxy Tab 2 Firmware"
url="https://github.com/TheMuppets/proprietary_vendor_samsung"
arch="armhf"
license="proprietary"
source="$pkgname-$_commit.tar.gz::https://github.com/TheMuppets/proprietary_vendor_samsung/archive/$_commit.tar.gz";
options="!check !strip !archcheck !spdx"
builddir="$srcdir/proprietary_vendor_samsung-$_commit"

_files="
	bcmdhd_apsta.bin
	bcmdhd_p2p.bin
	bcmdhd_sta.bin
	nvram_net.txt
"

package() {
	for _i in $_files; do
		install -Dm644 espresso-common/proprietary/system/etc/wifi/"$_i" \
			"$pkgdir"/lib/firmware/postmarketos/"$_i"
	done
}
